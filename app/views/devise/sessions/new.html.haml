%h2.text-center Sign in
= bootstrap_form_for(resource, url: session_path(resource_name), layout: :horizontal, label_col: "col-sm-4 ", control_col: "col-sm-4") do |f|
  = devise_error_messages!
  = f.email_field :email
  = f.password_field :password
  = f.form_group do
    = f.check_box :remember_me
  = f.form_group do
    = f.submit "Sign in", class: "btn-primary"
    %br/
    = render "devise/shared/links"
